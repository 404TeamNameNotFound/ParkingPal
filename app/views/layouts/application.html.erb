<!--blabla-->
  <meta property="og:image"         content="http://i66.tinypic.com/2jbnp68.jpg" />
</head>
<body>



<nav class="navbar navbar-default navbar-static-top">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="/">
        ParkingPal
      </a>
    </div>
    <ul class="nav navbar-nav navbar-right">
      <% if current_user %>
          <p class="navbar-text">Welcome back, <%= current_user.email %>!</p>
          <li><%= link_to "Log out", logout_path, method: "delete" %></li>
          <% if current_user && current_user.admin? %>
              <li><a href="/update" onclick="return confirm('Are you sure you want to update the database? This may take some time.');">Update</a></li>
              <li><a href="/edit">Edit</a></li>
          <%end%>
      <% else %>
          <li><a href="/login">Login</a></li>
          <li><a href="/signup">Sign Up</a></li>
      <% end %>
    </ul>
  </div>
</nav>
<!-- <div class="container"> -->

<%= yield %>

<!-- </div> -->

